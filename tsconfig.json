{
  "extends": "@adonisjs/tsconfig/tsconfig.app.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build",
    "paths": {
      "#app/*": [
        "./app/*"
      ],
      "#services/*": [
        "./app/Services/*"
      ],
      "#utils/*": [
        "./app/utils/*"
      ],
      "#controllers/pasien/*": [
        "./app/Controllers/pasien/*"
      ],
      "#helpers/*": [
        "./app/helpers/*"
      ]
    }
  }
}